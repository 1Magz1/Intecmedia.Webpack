{
    "root": true,
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "jquery": true
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "no-console": "off",
        "no-param-reassign": [2, { "props": false }],
        "unicode-bom": ["error", "never"],
        "max-len": ["error", 120, 4],
        "indent": ["error", 4]
    }
}