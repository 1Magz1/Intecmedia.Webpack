{
    "extends": [
        "htmlvalidate:recommended",
        "htmlvalidate:document"
    ],
    "rules": {
        "long-title": ["error", {
            "maxlength": 70
        }],
        "input-missing-label": "off",
        "heading-level": "off",
        "no-inline-style": "off",
        "require-sri": "off",
        "svg-focusable": "off"
    }
}
