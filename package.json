{
    "name": "Intecmedia.Webpack",
    "version": "0.0.1",
    "description": "Intecmedia Webpack bootstrap",
    "repository": {
        "type": "git",
        "url": "https://github.com/Intecmedia/Intecmedia.Webpack.git"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEBUG=0 webpack -d",
        "debug": "cross-env NODE_ENV=development DEBUG=1 webpack -d",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack -p --optimize-minimize",
        "watch": "npm run watch-prod",
        "watch-dev": "cross-env NODE_ENV=development DEBUG=0 webpack -w -d",
        "watch-debug": "cross-env NODE_ENV=development DEBUG=1 webpack -w -d",
        "watch-prod": "cross-env NODE_ENV=production DEBUG=0 webpack -w -p --optimize-minimize",
        "js-lint": "cross-env NODE_ENV=development eslint --config .eslintrc.json --ignore-path .eslintignore --fix source/js/ --ext .js && eslint --config .eslintrc.json --fix source/helpers/*.js && eslint --config .eslintrc.json --fix *.js",
        "css-lint": "cross-env NODE_ENV=development stylelint source/**/*.scss --fix --syntax scss --formater verbose --config .stylelintrc",
        "css-format": "cross-env NODE_ENV=development stylefmt --recursive source/css/**/*.scss --config .stylelintrc",
        "html-lint": "cross-env NODE_ENV=development htmllint --rc .htmllintrc build/**/*.html",
        "lint": "npm run js-lint && npm run css-format && npm run css-lint && npm run html-lint",
        "nocache": "rm -rf ./node_modules/.cache",
        "server": "npm run server-prod",
        "server-dev": "cross-env NODE_ENV=development DEBUG=0 webpack-dev-server --hot --inline -d -w",
        "server-debug": "cross-env NODE_ENV=development DEBUG=1 webpack-dev-server --hot --inline -d -w",
        "server-prod": "cross-env NODE_ENV=production DEBUG=0 webpack-dev-server --hot --inline -p --optimize-minimize -w",
        "build": "npm-run-all nocache js-lint css-format css-lint prod html-lint",
        "start": "npm run build"
    },
    "author": "Dmitry Pyatkov",
    "license": "MIT",
    "browserslist": [
        "last 2 Edge versions",
        "last 2 Explorer versions",
        "last 2 Chrome versions",
        "last 2 ChromeAndroid versions",
        "last 2 Firefox versions",
        "last 2 FirefoxAndroid versions",
        "last 2 Opera versions",
        "last 2 iOS versions",
        "last 2 Safari versions"
    ],
