{
    "name": "Intecmedia.Webpack",
    "version": "0.0.2",
    "description": "Intecmedia Webpack bootstrap",
    "repository": {
        "type": "git",
        "url": "https://github.com/Intecmedia/Intecmedia.Webpack.git"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEBUG=0 webpack -d",
        "debug": "cross-env NODE_ENV=development DEBUG=1 webpack -d",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack -p",
        "watch": "npm run watch-prod",
        "watch-dev": "cross-env NODE_ENV=development DEBUG=0 webpack --watch -d",
        "watch-debug": "cross-env NODE_ENV=development DEBUG=1 webpack --watch -d",
        "watch-prod": "cross-env NODE_ENV=production DEBUG=0 webpack --watch -p",
        "js-lint": "cross-env NODE_ENV=development eslint --config .eslintrc --ignore-path .eslintignore --fix *.js source/**/*.js",
        "css-lint": "cross-env NODE_ENV=development stylelint source/**/*.scss --fix --syntax scss --formater verbose --config .stylelintrc",
        "css-format": "cross-env NODE_ENV=development stylefmt --recursive source/**/*.scss --config .stylelintrc",
        "html-lint": "cross-env NODE_ENV=development node ./script.html-lint.js",
        "lint": "npm run js-lint && npm run css-format && npm run css-lint && npm run html-lint",
        "server": "npm run server-dev",
        "server-dev": "cross-env NODE_ENV=development DEBUG=0 webpack-dev-server --hot --inline -d",
        "server-debug": "cross-env NODE_ENV=development DEBUG=1 webpack-dev-server --hot --inline -d",
        "build": "npm-run-all js-lint css-format css-lint prod html-lint",
        "browserslist": "npx browserslist",
        "start": "npm run build"
    },
    "author": "Dmitry Pyatkov",
    "license": "MIT",
    "browserslist": [
        "Android >= 5",
        "IE >= 10",
        "iOS >= 9",
        "Safari >= 9",
        "last 2 Edge versions",
        "last 2 Chrome versions",
        "last 2 ChromeAndroid version",
        "last 2 Firefox versions",
        "last 2 Opera versions"
    ],
