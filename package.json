{
    "name": "Intecmedia.Webpack",
    "version": "0.0.1",
    "description": "Intecmedia Webpack bootstrap",
    "repository": {
        "type": "git",
        "url": "https://github.com/Intecmedia/Intecmedia.Webpack.git"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEBUG=0 webpack -d",
        "debug": "cross-env NODE_ENV=development DEBUG=1 webpack -d",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack -p --optimize-minimize",
        "watch": "npm run watch-prod",
        "watch-dev": "cross-env NODE_ENV=development DEBUG=0 webpack -w -d",
        "watch-debug": "cross-env NODE_ENV=development DEBUG=1 webpack -w -d",
        "watch-prod": "cross-env NODE_ENV=production DEBUG=0 webpack -w -p --optimize-minimize",
        "js-lint": "cross-env NODE_ENV=development eslint --config .eslintrc.json --fix source/js/ --ext .js && eslint --config .eslintrc.json --fix *.js",
        "css-lint": "cross-env NODE_ENV=development stylelint source/**/*.scss --fix --syntax scss --formater verbose --config .stylelintrc",
        "css-format": "cross-env NODE_ENV=development stylefmt --recursive source/css/**/*.scss --config .stylelintrc",
        "html-lint": "cross-env NODE_ENV=development htmllint --rc .htmllintrc build/**/*.html",
        "lint": "npm run js-lint && npm run css-format && npm run css-lint && npm run html-lint",
        "nocache": "rm -rf ./node_modules/.cache",
        "server": "npm run server-prod",
        "server-dev": "cross-env NODE_ENV=development DEBUG=0 webpack-dev-server -d --inline --hot --open",
        "server-debug": "cross-env NODE_ENV=development DEBUG=1 webpack-dev-server -d --inline --hot --open",
        "server-prod": "cross-env NODE_ENV=production DEBUG=0 webpack-dev-server -p --inline --hot --open --optimize-minimize",
        "build": "npm run nocache && npm run js-lint && npm run css-format && npm run css-lint && npm run prod && npm run html-lint",
        "start": "npm run build"
    },
    "author": "Dmitry Pyatkov",
    "license": "MIT",
    "browserslist": [
        "Android >= 4",
        "Chrome >= 35",
        "Firefox >= 38",
        "Explorer >= 10",
        "Edge >= 12",
        "iOS >= 8",
        "Safari >= 8"
    ],
    "dependencies": {
        "autoprefixer": "latest",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-eslint": "latest",
        "babel-loader": "latest",
        "babel-polyfill": "latest",
        "babel-preset-airbnb": "latest",
        "babel-preset-env": "latest",
        "bootstrap-sass": "latest",
        "browserslist": "latest",
        "cross-env": "latest",
        "css-hot-loader": "latest",
        "css-loader": "latest",
        "css-mqpacker": "latest",
        "eslint": "latest",
        "eslint-config-airbnb-base": "latest",
        "eslint-loader": "latest",
        "eslint-plugin-import": "latest",
        "extract-text-webpack-plugin": "latest",
        "file-loader": "latest",
        "flat-cache": "latest",
        "font-awesome-sass": "latest",
        "glob": "latest",
        "handlebars": "latest",
        "handlebars-loader": "latest",
        "html-loader": "latest",
        "html-webpack-plugin": "latest",
        "htmllint-cli": "latest",
        "imagemin": "latest",
        "imagemin-gifsicle": "latest",
        "imagemin-jpegtran": "latest",
        "imagemin-optipng": "latest",
        "imagemin-pngquant": "latest",
        "imagemin-svgo": "latest",
        "imports-loader": "latest",
        "jquery": "latest",
        "json-loader": "latest",
        "loader-utils": "latest",
        "modernizr": "latest",
        "modernizr-loader": "latest",
        "node-sass": "latest",
        "picturefill": "latest",
        "postcss": "latest",
        "postcss-cssnext": "latest",
        "postcss-loader": "latest",
        "postcss-sorting": "latest",
        "sass-loader": "latest",
        "sprintf-js": "latest",
        "slash": "latest",
        "style-loader": "latest",
        "stylefmt": "latest",
        "stylefmt-loader": "latest",
        "stylelint": "latest",
        "stylelint-config-sass-guidelines": "latest",
        "stylelint-no-unsupported-browser-features": "latest",
        "stylelint-scss": "latest",
        "stylelint-selector-bem-pattern": "latest",
        "stylelint-webpack-plugin": "latest",
        "url-loader": "latest",
        "webpack": "latest",
        "webpack-dev-server": "latest",
        "webpack-notifier": "latest"
    },
    "devDependencies": {}
}
